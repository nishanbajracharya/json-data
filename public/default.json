{
  "meta": {
    "name": "Default",
    "rootId": "app",
    "style": "",
    "stylePaths": ["public/style.css"]
  },
  "data": {
    "header": {
      "tag": "header",
      "class": "header",
      "contents": [
        {
          "tag": "h1",
          "class": "header-title",
          "contents": "John Doe"
        },
        {
          "class": "header-subtitle",
          "contents": [
            {
              "class": "header-link",
              "contents": "{{{image}}} Email {{{value}}}",
              "props": {
                "value": "<a href=\"mailto:#\" rel=\"noopenner noreferrer\" target=\"_blank\">johndoe@example.com</a>",
                "image": "<i class=\"fas fa-envelope\"></i>"
              }
            },
            {
              "class": "header-link",
              "contents": "{{{image}}} Github {{{value}}}",
              "props": {
                "value": "<a href=\"#\" rel=\"noopenner noreferrer\" target=\"_blank\">johndoe</a>",
                "image": "<i class=\"fab fa-github\"></i>"
              }
            }
          ]
        }
      ]
    },
    "body": {
      "tag": "main",
      "class": "main",
      "contents": [{
        "class": "section",
        "contents": [{
          "tag": "h2",
          "class": "section-title",
          "contents": "Section Title 1"
        }, {
          "class": "section-body",
          "contents": "Section Body Text"
        }]
      }, {
        "class": "section",
        "contents": [{
          "tag": "h2",
          "class": "section-title",
          "contents": "Section Title 2"
        }, {
          "class": "section-body",
          "contents": [{
            "tag": "ul",
            "class": "section-list",
            "contents": [{
              "tag": "li",
              "class": "section-list-item",
              "contents": "Section Item 1"
            }]
          }]
        }]
      }, {
        "class": "section",
        "contents": [{
          "tag": "h2",
          "class": "section-title",
          "contents": "Section Title 3"
        }, {
          "class": "section-body",
          "contents": [{
            "tag": "ul",
            "class": "section-list",
            "contents": [{
              "tag": "li",
              "class": "section-list-item",
              "contents": "Section Item 1"
            }, {
              "tag": "li",
              "class": "section-list-item",
              "contents": "Section Item 2"
            }]
          }]
        }]
      }]
    },
    "footer": {
      "tag": "footer",
      "class": "footer",
      "contents": [
        {
          "class": "footer-title",
          "contents": "Created using {{{link}}}",
          "props": {
            "link": "<a href=\"https://github.com/nishanbajracharya/json-document\" target=\"_blank\" rel=\"noopenner noreferrer\">JSON Document</a>"
          }
        }
      ]
    }
  }
}
